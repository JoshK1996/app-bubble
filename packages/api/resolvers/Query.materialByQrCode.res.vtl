## If error is returned in the response
#if($ctx.error)
  $utils.error($ctx.error.message, $ctx.error.type, $ctx.result)
#end

## If the response is null, return an empty result
#if($util.isNull($ctx.result))
  #return({
    "items": [],
    "nextToken": null
  })
#end

## Otherwise, return the result directly
$util.toJson($ctx.result) 